{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": false,
        "no-bitwise": false,
        "max-classes-per-file": false,
        "no-namespace": false,
        "quotemark": {
            "options": "single"
        }
    },
    "rulesDirectory": []
}
